pipeline
{
   agent any
   stages
   { 
       stage('scm checkout')
       {
           steps {
               git branch: 'master', url: 'https://github.com/amruta-patinge/maven-project'
                 }
       }

       stage('please compile code')
       { steps {
           withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
            sh 'mvn compile'
}
       }

       }
      
           stage('please test code')
       { steps {
           withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
            sh 'mvn test'
}
       }

       }
        stage('please build code')
       { steps {
           withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
            sh 'mvn package'
}
       }

       }
   } 
}
